# Configurações do Bug Finder System
# Copie este arquivo para .env e configure suas credenciais

# Configurações obrigatórias
GEMINI_API_KEY=your_gemini_api_key_here  # Chave da API do Google Gemini
GITHUB_TOKEN=your_github_token_here  # Token de acesso do GitHub
GITHUB_OWNER=your_github_owner_here  # Proprietário do repositório GitHub
GITHUB_REPOSITORY=your_github_repository_here  # Nome do repositório GitHub
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here  # URL do webhook do Discord

# Configurações opcionais
ENVIRONMENT=development  # Ambiente de execução (development/staging/production)
LOG_LEVEL=INFO  # Nível de log (DEBUG/INFO/WARNING/ERROR)
DISCORD_CRITICAL_CHANNEL=  # ID do canal para bugs críticos
DISCORD_HIGH_CHANNEL=  # ID do canal para bugs de alta prioridade
DISCORD_GENERAL_CHANNEL=  # ID do canal geral

# Configurações avançadas
DB_HOST=localhost
DB_PORT=5432
DB_NAME=bugfinder
DB_USERNAME=
DB_PASSWORD=
